# ----------------------------
# deal with list
# ----------------------------
>>> lst = [2, 3, 9, 6]
>>> pd.Series(lst)
0    2
1    3
2    9
3    6
dtype: int64

>>> pd.Series(lst, index=['a','b','c','d'])
a    2
b    3
c    9
d    6
dtype: int64

# ----------------------------
# deal with dict
# ----------------------------
>>> d = {'a':3, 'b':5, 'c': 2}
>>> pd.Series(d)
a    3
b    5
c    2
dtype: int64
